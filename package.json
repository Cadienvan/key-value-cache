{
  "name": "@cadienvan/key-value-cache",
  "private": false,
  "version": "0.4.0",
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "exports": {
    "import": "./dist/mjs/index.js",
    "require": "./dist/cjs/index.js",
    "default": "./dist/mjs/index.js"
  },
  "types": "dist/types/index.d.ts",
  "scripts": {
    "clean": "rm -fr ./dist",
    "build": "npm run clean && mkdir dist && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && cp package-cjs.json dist/cjs/package.json && cp package-mjs.json dist/mjs/package.json",
    "demo-test": "for file in demo/*.js; do node $file; done"
  },
  "prepublish": "tsc",
  "devDependencies": {
    "@types/node": "^18.11.7",
    "tsc": "^2.0.4",
    "typescript": "^4.8.4"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Cadienvan/key-value-cache.git"
  },
  "license": "MIT"
}
