{
  "name": "@cadienvan/key-value-cache",
  "private": false,
  "version": "0.6.2",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "ts-types": "tsc --emitDeclarationOnly",
    "build": "npm run lint && rimraf dist && node esbuild.js && npm run ts-types",
    "demo-test": "for file in demo/*.js; do node $file; done",
    "prettier": "prettier --write .",
    "lint": "eslint . --ext .ts",
    "prepare": "husky install",
    "dev": "jest --watch",
    "test": "jest --no-cache"
  },
  "prepublish": "npm run build",
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "esbuild": "^0.19.10",
    "eslint": "^8.56.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "npx prettier --write",
      "npx eslint --fix",
      "npx jest --no-cache"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Cadienvan/key-value-cache.git"
  },
  "license": "MIT",
  "author": "Cadienvan <cadienvan@gmail.com>",
  "contributors": [
    {
      "name": "Antonio Caputo",
      "url": "https://github.com/tonycaputome"
    }
  ],
  "dependencies": {
    "global": "^4.4.0"
  }
}
