    "esbuild": "^0.15.12",
    "eslint": "^8.26.0",
    "rimraf": "^3.0.2",
    "tsc": "^2.0.4",
    "typescript": "^4.8.4"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Cadienvan/key-value-cache.git"
  },
